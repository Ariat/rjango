{
  "name": "reango",
  "version": "1.0.0",
  "description": "reango",
  "scripts": {
    "nativeserver": "node node_modules/react-native/local-cli/cli.js start",
    "start": "babel-node ./config/server.js",
    "ios": "react-native run-ios",
    "build": "yarn run clean && NODE_ENV=production webpack",
    "lint": "eslint client && prospector src--uses django --ignore-patterns */migrations",
    "lint-fix": "eslint --fix client && autopep8 --in-place --aggressive --aggressive --recursive --max-line-length 79 server",
    "clean": "rm -rf ./static/bundles/* ./static/index.html",
    "postinstall": "python3 ./manage.py graphql_schema && npm run build",
    "relay": "relay-compiler --watch --src ./client --schema ./lib/schema.json",
    "flow-server": "flow server",
    "flow": "flow status"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ncrmro/reango.git"
  },
  "engines": {
    "node": ">=8.7.0",
    "npm": "5.0.3"
  },
  "author": "Nicholas Romero",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-plugin-relay": "^1.4.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "webpack-dev-server": "^2.9.4",
    "webpack-hot-middleware": "^2.21.0"
  },
  "dependencies": {
    "base-64": "^0.1.0",
    "connect-history-api-fallback": "^1.3.0",
    "cross-env": "^3.1.3",
    "css-loader": "^0.28.7",
    "dotenv": "^4.0.0",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "lodash": "^4.17.2",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-hot-loader": "3.0.0-beta.6",
    "react-mdc-web": "^0.15.0",
    "react-native": "0.50.*",
    "react-native-web": "^0.3.2",
    "react-native-windows": "0.50.0",
    "react-relay": "^1.4.1",
    "react-relay-network-layer": "^2.0.1",
    "react-router-dom": "^4.2.2",
    "react-router-native": "^4.2.0",
    "relay-compiler": "^1.4.1",
    "relay-query-lookup-renderer": "^2.0.3",
    "relay-runtime": "^1.4.1",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.9.1",
    "webpack-bundle-tracker": "^0.2.0",
    "webpack-pwa-manifest": "^3.3.2"
  },
  "metadata": {
    "graphql": {
      "schema": "./build/schema.json"
    }
  }
}
